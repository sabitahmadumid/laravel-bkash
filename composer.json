{
    "name": "sabitahmad/laravel-bkash",
    "description": "Unofficial Bkash Package for Laravel providing seamless integration with Bkash payment gateway.",
    "keywords": [
        "SabitAhmad",
        "laravel",
        "laravel-bkash"
    ],
    "homepage": "https://github.com/sabitahmad/laravel-bkash",
    "license": "MIT",
    "authors": [
        {
            "name": "Sabit Ahmad",
            "email": "sabitahmadumid@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0"

    },
    "autoload": {
        "psr-4": {
            "SabitAhmad\\Bkash\\": "src/",
            "SabitAhmad\\Bkash\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SabitAhmad\\Bkash\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SabitAhmad\\Bkash\\BkashServiceProvider"
            ],
            "aliases": {
                "Bkash": "SabitAhmad\\Bkash\\Facades\\Bkash"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
